{
  "name": "valercodes/fever-code-challenge-plugin",
  "description": "Fever Code Challenge WordPress Plugin",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "version": "0.1.0",
  "vendor-dir": "vendor",
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "brain/monkey": "^2.6",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "FeverCodeChallenge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FeverCodeChallenge\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpcs": "phpcs --standard=phpcs.xml",
    "phpcbf": "phpcbf --standard=phpcs.xml",
    "pot": "wp i18n make-pot . languages/fever-code-challenge.pot --exclude=node_modules,vendor,build --allow-root",
    "test": "phpunit"
  }
}